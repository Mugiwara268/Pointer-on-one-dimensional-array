#include <iostream> 

using namespace std;

int main() {

    int num[5] = { 1,2,3,4,5 }; //one dimensional array (5 numbers)

    int* ptr = num; //pointer on one dimensional array


    for (int i = 0; i < 5; i++) {

        cout << "Place: " << i << " number: " << *(ptr + i) << endl; //output with pointer 
    }

    return 0;//no return
}

